{
  "name": "FocusOn",
  "description" : "An interactive to do list that helps you FocusOn what to do next",
  "version": "0.6.0",
  "manifest_version": 2,
  "background": {
      "persistent": false,
      "scripts": ["background.js"]
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },    
  "browser_action": {
    "default_title": "FocusOn"
  },
  "permissions": [
    "storage"
  ]   
}
