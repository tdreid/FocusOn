{
  "name": "FocusOn",
  "description" : "Chrome Extension to help focus on items to do",
  "version": "0.2.0",
  "manifest_version": 2,
  "background": {
      "persistent": false,
      "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "FocusOn"
  },
  "permissions": [
    "storage"
  ]   
}
